    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/swagger": "^7.1.2",
    "@prisma/client": "^5.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/passport": "^1.0.12",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
